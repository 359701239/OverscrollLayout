<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wcy.overscrolllayout">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.wcy.overscrolllayout.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.wcy.overscrolllayout.ListViewActivity"></activity>
        <activity android:name="com.wcy.overscrolllayout.GridViewActivity"></activity>
        <activity android:name="com.wcy.overscrolllayout.ScrollViewActivity"></activity>
        <activity android:name="com.wcy.overscrolllayout.HorizontalScrollViewActivity"></activity>
        <activity android:name="com.wcy.overscrolllayout.WebViewActivity"></activity>
        <activity android:name="com.wcy.overscrolllayout.PagerViewActivity"></activity>
        <activity android:name="com.wcy.overscrolllayout.CustomViewActivity"></activity>
        <activity
            android:name="com.wcy.overscrolllayout.RecyclerViewActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
